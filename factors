#!/bin/bash

if [ $# -ne 1 ]; then
    echo "Usage: $0 <file>"
    exit 1
fi

input_file="$1"

while read -r number; do
    factorize=""
    n=$number

    for ((i = 2; i <= n / 2; i++)); do
        if ((n % i == 0)); then
            factorize="$i*$((n / i))"
            break
        fi
    done

    echo "$n=$factorize"
done < "$input_file"

